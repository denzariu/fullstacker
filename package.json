{
  "name": "fullstacker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "npm-run-all --parallel watch:server start:web",
    "start:web": "npm run dev",
    "start:server": "ts-node src/server/index.ts",
    "watch:server": "nodemon src/server/index.ts",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@react-oauth/google": "^0.12.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "20.11.24",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-graphql": "^0.12.0",
    "formik": "^2.4.5",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jwt-decode": "^4.0.0",
    "mysql2": "^3.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "urql": "^4.0.6",
    "wouter": "^3.0.2",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  },
  "eslintConfig": {
    "globals": {
      "google": true
    }
  }
}
